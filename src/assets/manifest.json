{
  "manifest_version": 3,
  "name": "Saver",
  "version": "1.0.0",
  "background": {
    "service_worker": "js/background.js"
  },
  "icons": {
    "48": "img/icon-48.png",
    "120": "img/icon-120.png",
    "512": "img/icon-512.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "js/index.html",
    "default_title": "React Chrome Extension"
  }
}
